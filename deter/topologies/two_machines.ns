set ns [new Simulator]
source tb_compat.tcl

set bridge [$ns node]
tb-set-node-os $bridge Ubuntu1804-STD

set nodeA [$ns node]
tb-set-node-os $nodeA Ubuntu1804-STD

set nodeB [$ns node]
tb-set-node-os $nodeB Ubuntu1804-STD

set lan [$ns make-lan "bridge nodeA nodeB" 1000Mb 0ms]

$ns rtproto Static
$ns run